<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.riabilitazione">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Riabilitazione">
        <activity
            android:name=".DettagliEsercizi"
            android:exported="false"
            android:label="@string/title_activity_dettagli_esercizi"
            android:theme="@style/Theme.Riabilitazione.NoActionBar" />
        <activity
            android:name=".DettagliAllenamento"
            android:exported="false" />
        <activity
            android:name=".VotoDifficolta"
            android:exported="false" />
        <activity
            android:name=".AllenamentoPaziente"
            android:exported="false" />
        <activity
            android:name=".ProgressoPaziente"
            android:exported="false" />
        <activity
            android:name=".informazionipersonali"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>